@model List<NPGeek.Web.Models.Park>

@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}




<title>Home</title>

<div>

    @foreach (var park in Model)
    {
        <div class="park">
            <img src="~/Content/parks/@(park.ParkCode).jpg" />
            <a href="@Url.Action("Detail","Park", new { parkCode = park.ParkCode })">@park.Name</a>
            <h4>@park.State</h4>
            <p>@park.ParkDescription</p>
        </div>
    }

</div>
